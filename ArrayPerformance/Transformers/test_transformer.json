{
  "test_non_existing": {
    "#ifgroup(#exists($.RECORDNO))": {
      "record_no": "#valueof($.RECORDNO)"
    },
    "#ifgroup(#exists($.CREATEDBY))": {
      "created_by": "#valueof($.CREATEDBY)"
    },
    "#ifgroup(#exists($.MODIFIEDBY))": {
      "modified_by": "#valueof($.MODIFIEDBY)"
    },
    "array_data": {
      "#loop($.ARRAYDATA)": {
        "no_check_record_no": "#currentvalueatpath($.RECORDNO)",
        "no_check_created_by": "#currentvalueatpath($.CREATEDBY)",
        "no_check_modified_by": "#currentvalueatpath($.MODIFIEDBY)",
        "#ifgroup(#exists($.RECORDNO))": {
          "record_no": "#currentvalueatpath(.RECORDNO)"
        },
        "#ifgroup(#exists($.CREATEDBY))": {
          "created_by": "#currentvalueatpath(.CREATEDBY)"
        },
        "#ifgroup(#exists($.MODIFIEDBY))": {
          "modified_by": "#currentvalueatpath(.MODIFIEDBY)"
        }
      }
    }
  }
}